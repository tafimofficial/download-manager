{
    "manifest_version": 3,
    "name": "Turbo Downloader Integration",
    "version": "1.0",
    "description": "Catch downloads directly from Chrome with Turbo Downloader.",
    "permissions": [
        "downloads",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon128.png"
    }
}